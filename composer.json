{
	"name": "cnvs/canvas",
	"description": "A blogging package for Laravel.",
	"keywords": [
		"cnvs",
		"canvas",
		"blog",
		"laravel"
	],
	"license": "MIT",
	"homepage": "https://cnvs.io",
	"support": {
		"issues": "https://github.com/cnvs/canvas/issues",
		"source": "https://github.com/cnvs/canvas"
	},
	"authors": [
		{
			"name": "Todd Austin",
			"email": "austin.todd.j@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.1.3",
		"ext-json": "*",
		"doctrine/dbal": "^2.6",
		"illuminate/database": "^5.7",
		"illuminate/support": "^5.7"
	},
	"require-dev": {
		"mockery/mockery": "^1.1",
		"orchestra/testbench": "~3.7.0",
		"phpunit/phpunit": "^7.1"
	},
	"autoload": {
		"psr-4": {
			"Canvas\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Canvas\\Tests\\": "tests"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Canvas\\CanvasServiceProvider"
			]
		}
	}
}
