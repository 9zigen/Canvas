{
	"name": "cnvs/canvas",
	"description": "A Laravel 5 package to add a blog to your application.",
	"keywords": ["cnvs", "canvas", "blog", "laravel"],
	"license": "MIT",
	"homepage": "https://cnvs.io",
	"support": {
		"issues": "https://github.com/cnvs/canvas/issues",
		"source": "https://github.com/cnvs/canvas"
	},
	"authors": [
		{
			"name": "Todd Austin",
			"email": "austin.todd.j@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.1.3",
		"doctrine/dbal": "^2.6",
		"illuminate/database": "~5.7.0",
		"illuminate/support": "~5.7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.0"
	},
	"autoload": {
		"psr-4": {
			"Canvas\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Canvas\\Tests\\": "tests"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Canvas\\CanvasServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	}
}
